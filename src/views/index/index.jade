include ../../blocks/default.jade

doctype 5
html.no-js
    head
        +i-head('Index')
    body.b-page
        .b-header.b-container
            +b-logo('slogan')
        .b-page__body.b-container
            p.b-page__body_ask How do you want to choose a location?
            .b-chcontainer.b-container
                .b-chcontainer__item
                    a.b-link.b-chcontainer__item__link.b-chcontainer__item_search(href='/search')
                        img.b-chcontainer__item__img.b-chcontainer__item__img_search(src='data:image/svg+xml;base64,', alt='Search')
                        br
                        | Search
                    form(action='/search').b-chcontainer__item__form
                        input(type='search', name='q', required, autofocus).b-chcontainer__item__form__input
                        button(type='submit').b-chcontainer__item__form__button Search
                .b-chcontainer__item
                    a.b-link.b-chcontainer__item__link.b-chcontainer__item_loc(href='/location')
                        img.b-chcontainer-item-img.b-chcontainer__item__img_geoloc(src='data:image/svg+xml;base64,', alt='Geolocation')
                        br
                        | Geolocation
        +b-footer
        script(src='//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js')
        script(src='/js/global.min.js')
        +i-stat
